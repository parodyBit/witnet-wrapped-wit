{
  "name": "witnet-wrapped-wit",
  "version": "1.0.0",
  "author": "Witnet Foundation <info@witnet.foundation>",
  "description": "Wrapped/WIT Solidity contracts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/witnet/witnet-wrapped-wit"
  },
  "type": "commonjs",
  "exports": "./src/index.js",
  "bin": {
    "wrapped-wit-unwrapper": "src/bin/unwrapper.js"
  },
  "files": [
    "artifacts",
    "contracts",
    "src"
  ],
  "scripts": {
    "compile": "npx hardhat compile",
    "deploy": "npx hardhat run scripts/deploy.js",
    "prepare": "npx hardhat compile --force",
    "start": "node ./src/bin/unwrapper.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify": "npx hardhat verify"
  },
  "keywords": [
    "ethereum",
    "witnet",
    "wit/oracle",
    "$WIT",
    "token",
    "ERC-20"
  ],
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/community-contracts": "git+https://github.com/OpenZeppelin/openzeppelin-community-contracts.git#master",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "hardhat": "^2.26.1"
  },
  "dependencies": {
    "@witnet/sdk": "^1.0.6",
    "witnet-solidity-bridge": "~2.1.10",
    "dotenv": "^16.4.1",
    "ethers": "^6.15.0",
    "lodash.merge": "^4.6.2"
  }
}
